{
  "name": "okclaire",
  "version": "0.2.0",
  "description": "Bot for Real Estate",
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "build": "rm -rf dist && babel src --out-dir dist",
    "watch": "(npm run build -- --watch &) && sleep 3 && nodemon -w dist dist/main.js",
    "deploy": "npm run build && modulus deploy -p okclaire",
    "tunnel": "ngrok http $(node -e \"console.log(require('./dist/config').default.port)\")",
    "lint": "eslint src",
    "test": "(npm run build -- --watch &) && sleep 3 && ava --watch",
    "coverage": "npm run build && nyc ava",
    "report": "nyc report --reporter=html && open coverage/index.html"
  },
  "dependencies": {
    "babel-runtime": "6.11.6",
    "bluebird": "3.4.1",
    "forecast-io": "1.0.2",
    "got": "6.3.0",
    "koa": "2.0.0",
    "koa-router": "7.0.1",
    "mixpanel": "0.4.0",
    "mongoose": "4.5.8",
    "morgan": "1.7.0",
    "raw-body": "2.1.7",
    "source-map-support": "0.4.2",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "babel-cli": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-plugin-default-import-checker": "1.0.8",
    "babel-plugin-source-map-support-for-6": "0.0.5",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.11.6",
    "eslint": "2.13.1",
    "eslint-config-semistandard": "6.0.2",
    "eslint-config-standard": "5.3.5",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-react": "5.2.2",
    "eslint-plugin-standard": "1.3.3",
    "espower": "1.3.2",
    "ngrok": "2.2.2",
    "nyc": "8.0.0",
    "supertest": "2.0.0",
    "supertest-as-promised": "4.0.0"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "source": [
      "dist/**/*.js"
    ],
    "concurrency": 5,
    "require": "babel-register",
    "babel": "inherit"
  }
}
