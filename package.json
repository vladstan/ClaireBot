{
  "name": "clairebot",
  "version": "0.2.0",
  "description": "Bot for Real Estate",
  "main": "build/index.js",
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "build": "babel src --out-dir build",
    "watch": "npm run build && (npm run build -- --watch &) && nodemon -w build build/index.js",
    "deploy": "npm run build && modulus deploy"
  },
  "dependencies": {
    "babel-runtime": "6.11.6",
    "forecast-io": "1.0.2",
    "got": "6.3.0",
    "koa": "2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-plugin-default-import-checker": "1.0.8",
    "babel-plugin-source-map-support-for-6": "0.0.5",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-preset-es2015": "6.13.1",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "2.13.1",
    "eslint-config-semistandard": "6.0.2",
    "eslint-config-standard": "5.3.5",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-react": "5.2.2",
    "eslint-plugin-standard": "1.3.3",
    "modulus": "6.1.1"
  }
}
