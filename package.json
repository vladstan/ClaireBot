{
  "name": "okclaire",
  "version": "0.2.0",
  "description": "Bot for Real Estate",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "build": "babel src --out-dir dist",
    "watch": "(npm run build -- --watch &) && sleep 3 && nodemon -w dist dist/server.js",
    "deploy": "npm run build && modulus deploy -p okclaire",
    "tunnel": "ngrok http $(node -e \"console.log(require('./dist/config').default.port)\")",
    "lint": "eslint src"
  },
  "dependencies": {
    "babel-runtime": "6.11.6",
    "bluebird": "3.4.1",
    "forecast-io": "1.0.2",
    "got": "6.3.0",
    "koa": "2.0.0",
    "koa-router": "7.0.1",
    "mixpanel": "0.4.0",
    "mongoose": "4.5.8",
    "morgan": "1.7.0",
    "raw-body": "2.1.7",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-plugin-default-import-checker": "1.0.8",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "2.13.1",
    "eslint-config-semistandard": "6.0.2",
    "eslint-config-standard": "5.3.5",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-react": "5.2.2",
    "eslint-plugin-standard": "1.3.3",
    "ngrok": "2.2.2"
  }
}
